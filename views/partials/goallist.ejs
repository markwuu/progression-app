<div class="column">
    <div>completed goals:</div>
    <div class="row">

        <% for (let i = 0; i < completedGoals.length; i++) { %>
        <div class="col-md-4">
            <div class="card card-body text-center bg-info">
                <a href="/goals/<%= completedGoals[i] %>/<%= user.username %>">
                    <%= completedGoals[i] %>
            </div>
        </div>
        </a>
        <% } %>
    </div>
</div>
<div class="column">
    <div>uncompleted goals:</div>
    <div class="row">
        <% for (let i = 0; i < uncompletedGoals.length; i++) { %>
        <div class="col-md-4">
            <div class="card card-body text-center">
                <a href="/goals/<%= uncompletedGoals[i] %>/<%= user.username %>">
                    <%= uncompletedGoals[i] %>
                </a>
            </div>
        </div>
        <% } %>
    </div>
</div>
